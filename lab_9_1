#include <stdio.h>
#include <stdbool.h>
/*Задача №48 на СИ: Программа выполнения логической операции "штрих Шеффера" (Отрицание конъюнкции) */
bool ShtrihSheffera(bool a, bool b) {
    return !(a && b);
}

int main() {
    int a, b;
    bool res;
    printf("Программа выполнения логической операции \"штрих Шеффера\" \n");
    printf("Введите a (0 или 1) \n");
    scanf("%d", &a);
    printf("Введите b (0 или 1) \n");
    scanf("%d", &b);
    
    res = ShtrihSheffera((bool)a, (bool)b);
    printf("Результат операции: %d", res);
}

