#include <iostream>
using namespace std;
/*Задача №14 на С++ Решение с использованием цикла с постусловием: Найти сумму чисел от 100 до 200, кратных 17*/
int main() {
    cout <<"Пргорамма выводит сумму чисел от 100 до 200, кратных 17"<<endl;
    int s= 0, i = 100;

    do {
        if (i % 17 == 0) {
            s += i;
        }
        i++;
    } 
    while (i <= 200);
    cout << "Сумма чисел от 100 до 200, кратных 17- "<<s;
}
