#include <stdio.h>
#include <math.h>
/*Задача №50 на си: Имеется последовательность чисел а1, а2, … , аN. Определите квадрат суммы положительных членов этой последовательности*/
int main() {
    int s=0, i=0, n, sq;
    printf("Программа выводит квадрат суммы положительных членов последовательности а1, а2, … , аN. %d\n");
    printf("Введите количество членов последовательности");
    scanf("%d", &n);
    int a[n];

    printf("Введите элементы последовательности:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &a[i]);
        }
        
    do {
        if (a[i] > 0) {
            s += a[i];
        }
    i++;
    } while (i < n);

    sq= s * s;  
    printf("Квадрат суммы положительных членов последовательности: %d\n", sq);
}

