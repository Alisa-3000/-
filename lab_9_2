#include <iostream>
#include <string>
#include <algorithm>
using namespace std;
/*Задача №4 на С++: Программа выполнения логической операции "штрих Шеффера" (Отрицание конъюнкции) */

bool Palindrom(int num) {
    string nstr = to_string(num);
    string r = nstr;
    reverse(r.begin(), r.end());
    return nstr == r;
}

int main() {
    int num;
    cout << "Введите число \n";
    cin >> num;

    if (Palindrom(num)) {
        cout << "Палиндром" << endl;
    } else {
        cout << "Не палиндром" << endl;
    }
}
